	private Database() 
	{
		System.out.println("Database is successfully Created");   
	}
	
	public static Databse getDatabse()
	{
		if(database == null)
	 database = new Login();
		return database;
	}
	
	public void status() 
	{
		System.out.println("Databse is successfully Connected");   
	}

}


}
